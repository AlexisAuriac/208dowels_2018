#!/bin/ocaml

Printf.printf "%d: %s\n" 5 "abc";;

Printf.printf "%d\n" (int_of_string "56");;
Printf.printf "%.2f\n" (float_of_string "56.78");;

Printf.printf "%d = %d -> %B\n" 7 8 (7 = 8);;
Printf.printf "%d <> %d -> %B\n" 7 8 (7 <> 8);;
(* print_bool true *)
(* print_newline ();; *)

